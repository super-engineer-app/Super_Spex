		"lint-staged": "^16.2.7",
		"typescript": "~5.9.2"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx}": [
			"biome check --write --no-errors-on-unmatched"
		],
		"*.json": [
			"biome check --write --no-errors-on-unmatched"
		]
	},
	"private": true
